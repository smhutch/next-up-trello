{
  "name": "Next Up Trello",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "See your due Trello tasks in each new tab.",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "identity",
    "*://*.trello.com/",
    "webNavigation",
    "*://*.segment.com/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
        "*://*.trello.com/",
        "chrome-extension://*",
        "index.html",
        "/dist/build.js"
    ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png" 
  } 
}
